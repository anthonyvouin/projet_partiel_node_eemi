server {
    listen {{ http_port }};
    server_name {{ full_front_hostname }};
    access_log /var/log/nginx/{{ full_front_hostname }}.access.log;
    error_log /var/log/nginx/{{ full_front_hostname }}.error.log;

    location / {
        root /var/www/html/{{ hostname }}/front;
        try_files $uri $uri/ /index.html;
    }
}
